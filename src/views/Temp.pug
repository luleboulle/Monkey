doctype
html(lang='fr')
	head
		meta( charset='utf-8' )
		title Monkey Manager 3000
		link(rel="shortcut icon", type="image/x-icon", href="../Content/Logo/Logo-Link.png")

		link(rel="stylesheet", href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css")
		
		script(src='https://code.jquery.com/jquery-3.3.1.slim.min.js')
		script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js')
		script(src='https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js')
		
		style
			include ../Css/StyleSheet.css

		link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css")

		link(href="https://fonts.googleapis.com/css?family=Great+Vibes", rel="stylesheet")

		script(src="../JS/Script.js")

		script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js")

		script.
			$(document).ready(function () {
				$('.js-scrollTo').on('click', function () { // Au clic sur un élément
					var page = $(this).attr('href'); // Page cible
					var speed = 750; // Durée de l'animation (en ms)
					$('html, body').animate({ scrollTop: $(page).offset().top }, speed); // Go
					return false;
				});
			});

	body
		nav(class="navbar navbar-expand-lg navbar-light bg-light  fixed-top", style="background-color: #ffffff;")
			a(class="navbar-brand justify-content-start d-flex", href="#")
				img( src="../Content/Logo/Logo-Texte.png", alt="Logo", style="height:30%; width:30%")

		h1 Monkey Manager 3000


		ul
			each val in Monkey
				li= val.name

		img(src="/static/Logo/Logo-Texte.png")
		img(src="https://vignette.wikia.nocookie.net/tokyoghoul/images/6/69/Juzo_Suzuya_Manga_-_Re.png/revision/latest?cb=20171214155757&path-prefix=fr")